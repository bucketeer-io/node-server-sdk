{
  "compilerOptions": {
    "target": "es2020",
    "module": "CommonJS",
    "moduleResolution": "node",
    "pretty": true,
    "strict": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "noUnusedParameters": true,
    "allowSyntheticDefaultImports": true,
    "preserveConstEnums": true,
    "esModuleInterop": true,
    "outDir": "__build",
    "types": [
      "node"
    ]
  },
}